<odoo>
  <data>

    <!-- Manager Request Confirm tree View  -->
    <record model="ir.actions.act_window" id="orphans_request_tree_action">
      <field name="name">Orphans Request window</field>
      <field name="res_model">orphans.request</field>
      <field name="view_mode">tree</field>
      <field name="domain">[('o_organization', '=', 'o_organization')]</field>
      <field name="view_id" ref="orphans_request_tree_view"/>
    </record>


    <!-- actions Advertise Fore View  Manager -->
    <record model="ir.actions.act_window" id="orphans_advertise_action_window1">
      <field name="name">Orphans Advertise window</field>
      <field name="res_model">orphans.advertise</field>
      <field name="view_mode">form</field>
    </record>

    <!-- Expense in Manager / Member Window -->
    <record model="ir.actions.act_window" id="orphans_expense_action_window1">
      <field name="name">Orphans Expense window</field>
      <field name="res_model">orphans.organization.expense</field>
      <field name="view_mode">form</field>
    </record>

  </data>
</odoo>