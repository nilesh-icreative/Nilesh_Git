<odoo>
  <data>

    <template id="login_forms_page" name="Login Page">
      <t t-call="website.layout">
        <div class="container"><br/><br/>
          <form role="form" action="/create/contacts" method="POST">
            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>

            <div>

              <div>
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px">
                    <span>Contacts Name:</span>
                  </label>
                  <div>
                    <input style="width: 350px" t-att-value="name" id="name" class="form-control o_website_form_input" autocomplete="off"
                         type="text" name="name" required="required"/>
                    <br/>
                  </div>
                </div>
              </div>

              <div>
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px">
                    <span>Contacts Phone:</span>
                  </label>
                  <div>
                    <input style="width: 350px" t-att-value="phone" id="phone" class="form-control o_website_form_input" autocomplete="off"
                         type="text" name="phone" maxlength="10" required="required"/>
                    <br/>
                  </div>
                </div>
              </div>

              <div>
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px">
                    <span>Contacts Email:</span>
                  </label>
                  <div>
                    <input style="width: 350px" t-att-value="email" id="email" autocomplete="off"
                         type="email" class="form-control o_website_form_input" name="email" required="required"/>
                    <br/>
                  </div>
                </div>
              </div>

              <div>
                <div class="row s_col_no_resize s_col_no_bgcolor">
                  <label class="col-form-label col-sm-auto s_website_form_label" style="width: 200px">
                    <span>Contacts Image:</span>
                  </label>
                  <div>
                    <input style="width: 350px" t-att-value="image_1920" id="image_1920" autocomplete="off"
                           data-show-upload="true" data-show-caption="true" lass="file" data-show-preview="true"
                           type="file" accept="image/*" name="image_1920" class="file"/>
                    <br/><br/>
                    <p>
                    <button style="width: 350px" type="text" class="btn-primary">Submit</button>
                    </p>
                    <br/><br/><br/>
                  </div>
                </div>
              </div>

            </div>
          </form>
        </div>
      </t>
    </template>

    <template id="contacts_create_successfully" name="Login Page">
      <t t-call="website.layout">
        <div class="container"><br/><br/>
          <h2 style="color: green">Successfully Created Record.</h2>
          <a t-attf-href="/contacts_details/">
            <button style="font-size: 15px;" class="btn-primary">Click</button>
          </a>
        </div>
      </t>
    </template>

  </data>
</odoo>
