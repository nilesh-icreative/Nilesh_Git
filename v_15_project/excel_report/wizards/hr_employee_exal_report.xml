<odoo>
  <data>

     <record id="hr_employee_wizards" model="ir.ui.view">
        <field name="name">Hr Employees Wizards</field>
        <field name="model">excel_report_wizards</field>
        <field name="arch" type="xml">
          <form>
            <sheet>
              <group>
                <field name="hr_employees_ids" string="Employee"/>
              </group>
              <group>
                <group>
                  <field name="start_date" string="Start Date"/>
                </group>
                <group>
                  <field name="end_date" string="End Date"/>
                </group>
              </group>
              <footer>
                <button name="print_excel_report" type="object" string="Print" class="btn-primary"/>
                <button string="Cancel" special="cancel"/>
              </footer>
            </sheet>
          </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="hr_wizards_action_window">
      <field name="name">Excel Report</field>
      <field name="res_model">excel_report_wizards</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="hr_employee_wizards"/>
      <field name="target">new</field>
    </record>

    <menuitem name="Employee Timesheet Report" id="hr_employee_excel_report"
              parent="hr.hr_menu_hr_reports" action="hr_wizards_action_window"/>

  </data>
</odoo>